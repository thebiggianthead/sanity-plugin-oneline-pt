{
  "name": "sanity-plugin-oneline-pt",
  "version": "1.0.0",
  "description": "A custom input component for single line Portable Text",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/thebiggianthead/sanity-plugin-oneline-pt#readme",
  "bugs": {
    "url": "https://github.com/thebiggianthead/sanity-plugin-oneline-pt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thebiggianthead/sanity-plugin-oneline-pt.git"
  },
  "license": "MIT",
  "author": "Tom Smith <tom@sanity.io>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@portabletext/editor": "^1.45.3",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.0",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2",
    "@sanity/util": "^3.82.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@sanity/pkg-utils": "^7.2.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanity": "^3.82.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@sanity/ui": "^2",
    "react": "^18 || ^19",
    "sanity": "^3",
    "styled-components": "^6"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.12.3"
}
